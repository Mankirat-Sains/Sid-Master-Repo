{
    "name": "@sidos/frontend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "mkdir -p .nuxt && NODE_OPTIONS=\"--localstorage-file=.nuxt/localstorage.json\" nuxt dev",
        "build": "mkdir -p .nuxt && NODE_OPTIONS=\"--localstorage-file=.nuxt/localstorage.json\" nuxt build",
        "generate": "mkdir -p .nuxt && NODE_OPTIONS=\"--localstorage-file=.nuxt/localstorage.json\" nuxt generate",
        "preview": "mkdir -p .nuxt && NODE_OPTIONS=\"--localstorage-file=.nuxt/localstorage.json\" nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint .",
        "typecheck": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@apollo/client": "^3.13.8",
        "@speckle/objectloader2": "^2.26.8",
        "@speckle/viewer": "^2.20.2",
        "@vue/apollo-composable": "^4.2.2",
        "apollo-upload-client": "^18.0.1",
        "docx": "^8.5.0",
        "echo": "^0.1.9",
        "graphql": "^16.6.0",
        "nuxt": "^4.0.3",
        "pdf-lib": "^1.17.1",
        "pdfjs-dist": "^4.10.38",
        "vue": "^3.5.13",
        "vue-router": "^4.4.5"
    },
    "devDependencies": {
        "@nuxt/devtools": "^1.7.0",
        "@nuxt/eslint": "^1.1.0",
        "@nuxtjs/tailwindcss": "^6.12.2",
        "@types/node": "^20.14.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^9.0.0",
        "napi-postinstall": "^0.3.4",
        "node-gyp-build-optional-packages": "^5.2.2",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.5.0",
        "vue-tsc": "^2.2.10"
    }
}
